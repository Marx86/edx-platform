<div class="modal-transcript" aria-hidden="false" aria-labelledby="modal-transcript-label" tabindex="-1" role="dialog">
    <div class="modal-body">
        <h2 id="modal-transcript-label"><%- gettext('Upload New Transcript') %></h2>
        <label for="select-transcript-languages"><%- gettext('Transcript Languages') %></label>
        <select id="select-transcript-languages" class="js-select-language" data-tooltip="<%- gettext('Pick transcript language') %>">
            <option></option>
            <% _.each(languages, function(language) { %>
            <option value="<%- language[0] %>"><%- language[1] %></option>
            <% }) %>
        </select>
    </div>
    <div class="modal-actions">
        <button class="close-button js-close-popup" role="button"><%- gettext('Cancel') %></button>
        <button class="next-button js-upload-transcript" disabled role="button" aria-disabled=true><%- gettext('Select file to upload')
        %></button>
    </div>
</div>
